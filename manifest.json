{
  "manifest_version": 2,
  "name": "Igihe Style Fixer",
  "version": "1.0",
  "description": "Improves UI for igihe.com",
  "icons": {
    "16": "icons/favicon-16x16.png",
    "32": "icons/favicon-32x32.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*.igihe.com/*"],
      "css": ["style.css"]
    }
  ]
}
